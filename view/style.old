/**/
.wp-crm-form-column input[name="close"] { float: left; }
.wp-crm-form-column input[name="next"] { float: right; }

/* Seller */
.ui-widget-seller {  border-radius: 6px; border: 2px solid rgb(189, 195, 199); background: #fff; position: absolute; z-index: 99999; }
.ui-widget-seller .ui-widget-seller-header { border-color: #eee; background-color: #eee; color: #000; margin: 1px; text-align: left; }
.ui-widget-seller-list { text-align: left; max-height: 350px; overflow: auto; }
.ui-widget-seller-list a { display: block; padding: 4px 16px 6px; background-color: #eee; margin: 1px; cursor: pointer; }
.ui-widget-seller-list a:hover { background-color: #39f; }

/* Buyer */
.ui-widget-buyer-wrapper { border-radius: 6px; padding: 2px; }
.ui-widget-buyer-switch { clear: both; display: block; }


.ui-widget-buyer { border-radius: 6px; border: 2px solid rgb(189, 195, 199); width: 100%; background: #fff; position: absolute; z-index: 99999; }
.ui-widget-buyer .ui-widget-buyer-header { color: #000; margin: 1px; text-align: left; }
.ui-widget-buyer-add { text-align: left; float: left; height: 440px; width: 50%; }
.ui-widget-buyer-add input[type="text"] { width: 60%; float: right; }
.ui-widget-buyer-add button { margin: 4px 24px 0; }
.ui-widget-buyer-add label { width: 30%; line-height: 40px; height: 40px; display: block; float: left; }
.ui-widget-buyer-add .radio { text-align: left; line-height: 24px; height: 24px; }
.ui-widget-buyer-list { text-align: left; float: right; height: 440px; overflow: auto; width: 50%; }
.ui-widget-buyer-list a { display: block; padding: 4px 16px 6px; background-color: #eee; margin: 1px; cursor: pointer; border-radius: 6px; }
.ui-widget-buyer-list a:hover { text-decoration: none; background-color: rgb(93, 173, 226); }
.widget-list-show {}
.widget-list-hide { display: none!important; }

/* Person */
.ui-widget-person { background: #fff; border-color: #fff; box-shadow: 1px 1px 1px #eee; position: absolute; z-index: 99999; }
.ui-widget-person .ui-widget-person-header { border-color: #eee; background-color: #eee; color: #000; margin: 1px; text-align: left; }
.ui-widget-person-add { text-align: left; float: left; height: 360px; width: 250px; }
.ui-widget-person-add input[type="text"] { margin: 0 16px; }
.ui-widget-person-add button { margin: 4px 24px 0; }
.ui-widget-person-add label { }
.ui-widget-person-list { text-align: left; float: right; height: 360px; overflow: auto; width: 250px; }
.ui-widget-person-list a { display: block; padding: 4px 16px 6px; background-color: #eee; margin: 1px; cursor: pointer; }
.ui-widget-person-list a:hover { background-color: #39f; }

/* Product */
.ui-widget-product { border-radius: 6px; border: 2px solid rgb(189, 195, 199); background: rgb(249, 248, 243); width: 60%; position: absolute; z-index: 99999; }
.ui-widget-product .ui-widget-product-header { border-color: #eee; background-color: #eee; color: #000; margin: 1px; text-align: left; }
.ui-widget-product-add { text-align: left; float: left; height: 300px; width: 50%; }
.ui-widget-product-add input[type="text"] { width: 60%; float: right; }
.ui-widget-product-add button { margin: 4px 24px 0; }
.ui-widget-product-add label {  width: 30%; float: left; line-height: 40px; height: 40px; display: block; }
.ui-widget-product-list { text-align: left; float: right; height: 300px; overflow: auto; width: 350px; }
.ui-widget-product-list input[type="text"] { width: 24px; margin: 0; text-align: right; }
.ui-widget-product-list div { margin: 1px 0; line-height: 24px; }
.ui-widget-product-list div button { float: right; height: 22px; }
.ui-widget-product-basket {}
.ui-widget-product-basket div { text-align: left; margin: 0; }
.ui-widget-product-basket input[type="text"] { width: 48px; margin: 0; display: inline; }
.ui-widget-product-basket button { margin: 0 0 0 24px; }

/* File */
.wp-crm-form-file { margin: 4px 15px 6px; text-align: left; }
.wp-crm-form-filedrop { }
.wp-crm-form-filedrop .fd-file { opacity: 0; font-size: 1px; position: absolute; right: 0; top: 0 margin: 0; width: 0; height: 0; cursor: pointer; filter: alpha(opacity=0); font-family: sans-serif; }
.wp-crm-form-filedrop-message { clear: both; width: 100%; text-align: center; display: block; padding: 4px 0; }
.wp-crm-form-filedrop-preview img { width: 10%; float: left; margin: 2px; }
.wp-crm-form-filedrop-progress { width: 85%; height: 24px; margin-top: 12px; overflow: hidden; float: right; border: 1px solid #eee; border-radius: 3px; }
.wp-crm-form-filedrop-progressbar { position: relative; height: 24px; background: #c00; width: 0; }
.wp-crm-form-hidden,
.wp-crm-form-hidden * { width: 0; height: 0; overflow: none; }

.wp-crm-form-filedrop-list { list-style-type: none; margin: 0; padding: 0; }
.wp-crm-form-filedrop-list li { margin: 0; padding: 0; clear: both; display: block; font-size: 8pt; line-height: 1.8em; height: 1.8em; overflow: hidden; position: relative; }
.wp-crm-form-filedrop-list input { margin-right: 0.5em; }
.wp-crm-form-filedrop-list i { position: absolute; right: 0; top: 0; line-height: 1.8em; height: 1.8em; cursor: pointer; }

/* RTE Input */
.tinyeditor { width: 100%; }
.wp-crm-form-textarea-rte-expand { cursor: pointer; }

/* Matrix Input */
.wp-crm-form-matrix-wrapper { margin: 4px 15px 6px; text-align: left; }
.wp-crm-form .wp-crm-form-matrix { margin: 0; padding: 0; width: 100%; }
.wp-crm-form-matrix>div { clear: both; margin: 0; padding: 0; }
.wp-crm-form-matrix .wp-crm-form-matrix-row { margin: 0; padding: 0; display: block; width: 100%; }
.wp-crm-form-matrix-row div { margin: 0; padding: 0; display: block; width: 66px; float: left; text-align: center!important; }
.wp-crm-form-matrix-row div input[type="text"] { margin: 4px 6px; }
.wp-crm-form-matrix .wp-crm-form-matrix-cell { }
.wp-crm-form-matrix .wp-crm-form-matrix-cell-add-row,
.wp-crm-form-matrix .wp-crm-form-matrix-cell-date { width: 120px; text-align: center; display: block; float: left; }
.wp-crm-form-matrix .btn { margin: 0!important; }

/* Buttons */
.wp-crm-form-login-buttons,
.wp-crm-form-forgot-buttons,
.wp-crm-form-buttons { margin: 1em 0; }

/* E-Invest Specific */
.wp-crm-form-separator { clear: both; }
.wp-crm-view-loading { position: absolute; left: 50%; width: 320px; height: 100px; background: #fff; border: 2px solid #eee; border-radius: 6px; box-shadow: 3px 3px 3px #eee;  margin-left: -160px; z-index: 9999; }
.wp-crm-view-loading-title { text-align: center; padding: 5px; font-weight: bold; display: block; }
.wp-crm-view-loading .progress { margin: 20px 9px 30px; width: 300px; }
.wp-crm-view-tree { width: 100%; height: 400px; }
.wp-crm-view-tree-menu { position: absolute; display: none; z-index: 100000; }

.wp-crm-view-folder-wrapper { clear: both; }
.wp-crm-view-folder { margin: 2px 0; display: block; clear: both; line-height: 40px; }
.wp-crm-view-folder:hover { text-decoration: none; }
.wp-crm-view-folder:before { font-size: 40px; margin-right: 5px; }
.wp-crm-view-folder-wrapper hr { clear: both; }

.wp-crm-selected { background: #78cd51!important; }
.wp-crm-inline-input { border: 0; border-bottom: 1px solid #eee; width: 100%; margin: 0; padding: 0; background: transparent; }

/****************** ALEX ADDONS *****************/
.wp-crm-view-data {
    display: inline-block;
    width: 100%;
    height: 100%;
}

/* E-Invest Course */
.wp-crm-view-course-video { }
.wp-crm-view-course-slide { }
.wp-crm-view-course-slideshow { position: relative; width: 100%; overflow: hidden; }
.wp-crm-view-course-slideshow-item { position: absolute; width: 100%; }
.wp-crm-view-course-slideshow-item img { width: 100%; }
.wp-crm-view-course-slideshow ul { position: absolute; left: 15px; bottom: 15px; z-index: 10; text-align: left; list-style-type: none outside none; padding: 0; margin: 0; }
.wp-crm-view-course-slideshow ul li { display: inline; cursor: pointer; padding: 5px; background: #fff; margin: 3px; border-radius: 3px; } 
.wp-crm-view-course-slideshow ul li.selected { background: #c00; color: #fff; }
.wp-crm-view-course-slideshow span { position: absolute; top: 50%; display: block; margin-top: -10px; padding: 10px; border-radius: 3px; background: #fff; cursor: pointer; }
.wp-crm-view-course-slideshow span.prev { left: 0; border-top-left-radius: 0; border-bottom-left-radius: 0; }
.wp-crm-view-course-slideshow span.next { right: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; }
.wp-crm-view-course-slideshow span.view { right: 15px; top: 15px; margin-top: 0; }
.wp-crm-view-course-slide-fullscreen { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 100; }
.wp-crm-view-course-slide-shadow { width: 100%; height: 100%; background: #000; opacity: .8; }
.wp-crm-view-course-slide-fullscreen img { position: absolute; top: 50px; left: 20%; width: 60%; }
.wp-crm-view-course-slide-fullscreen span { position: absolute; top: 50px; right: 20%; margin-right: -40px; display: block; padding: 10px; border-radius: 3px; background: #fff; cursor: pointer; }
.wp-crm-view-course-quiz ul { list-style-type: none; }
.wp-crm-view-course-quiz-question { display: block; }

/* Visual Aid */
.poscce { margin: 0 -20px 30px; padding: 0; border: 0; text-align: center; font-size: 8pt; font-weight: bold; }
.poscce img { margin: 0 0 20px; }
#content.front-page { background-image: url(images/background-ei.png); background-position: center center; }
.login-box { border-radius: 3px; }
.login-box .footer { font-size: 8pt; text-align: center; margin-top: 10px; }
